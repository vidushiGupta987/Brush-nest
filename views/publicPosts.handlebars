<div class="container">
  {{#each posts}}
    <div class="media">
  <img class="mr-3 post-user-pic" src="{{user.image}}">
  <div class="media-body">
    <h3 class="mt-0">{{user.fullname}}</h3>
<h5>{{title}} <small class="red"><i class="fa fa-globe"></i></small></h5>
<p>{{body}}</p>
<small>{{date}}</small>
  </div>
</div>
<hr>
{{else}}
<p>You are all caught up!!</p>
{{/each}}
</div>